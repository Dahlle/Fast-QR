{
    "manifest_version": 2,
    "name": "Quick-QR",
    "author": "Isak Dahl",
    "description": "Generate a QR code of the current page",
    "version": "1.0",
    "minimum_chrome_version": "46",
    "icons": {
        "128": "Icon-128.png",
        "48":  "Icon-48.png",
        "16": "Icon-16.png"
    },
    "browser_action":{
        "default_icon": "Icon-16.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}
